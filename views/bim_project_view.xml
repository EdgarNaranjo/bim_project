            <field name="res_model">documents.share</field>
            <field name="view_mode">tree</field>
        </record>

        <record id="action_picking_bim_all" model="ir.actions.act_window">
            <field name="name">Transfers</field>
            <field name="res_model">stock.picking</field>
            <field name="type">ir.actions.act_window</field>
            <field name="view_mode">tree,kanban,form,calendar</field>
            <field name="domain"></field>
            <field name="context">{'contact_display': 'partner_address', 'default_company_id': allowed_company_ids[0]}</field>
            <field name="search_view_id" ref="stock.view_picking_internal_search"/>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    No transfer found. Let's create one!
                </p><p>
                    Transfers allow you to move products from one location to another.
                </p>
            </field>
        </record>

        <record id="product_product_action_bim" model="ir.actions.act_window">
            <field name="name">Materials</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">product.product</field>
            <field name="view_mode">tree,form,kanban</field>
            <field name="search_view_id" ref="stock.stock_product_search_form_view"/>
            <field name="context">{"search_default_consumable": 1, 'default_detailed_type': 'product', 'default_resource_type': 'material'}</field>
            <field name="domain">[('detailed_type','!=','service'), ('resource_type', '=', 'material')]</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    No materials found. Let's create one!
                </p><p>
                    Track your stock quantities by creating storable materials.
                </p>
            </field>
        </record>

        <record id="stock_product_normal_bim_action" model="ir.actions.act_window">
            <field name="name">Material Variants</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">product.product</field>
            <field name="view_mode">tree,form,kanban</field>
            <field name="search_view_id" ref="stock.stock_product_search_form_view"/>
            <field name="context">{"search_default_consumable": 1, 'default_detailed_type': 'product'}</field>
            <field name="domain">[('detailed_type','!=','service')]</field>
        </record>

        <record id="product_manpower_action_bim" model="ir.actions.act_window">
            <field name="name">Manpower</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">product.product</field>
