<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!--sequence-->
        <record id="seq_bim_models_import" model="ir.sequence">
            <field name="name">Bim Models Import</field>
            <field name="code">bim.models.import</field>
            <field name="prefix">IMPORT/%(y)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_bim_project_project" model="ir.sequence">
            <field name="name">Project project</field>
            <field name="code">project.project</field>
            <field name="prefix">PRJ/%(y)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_bim_project_task" model="ir.sequence">
            <field name="name">Project task</field>
            <field name="code">project.task</field>
            <field name="prefix">TSK/%(y)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
